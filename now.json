{
    "version": 2,
    "builds": [
        {
            "src": "api/server.py",
            "use": "@sisp/python-wsgi",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/server.py"
        }
    ]
}