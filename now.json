{
    "version": 2,
    "builds": [{
            "src": "api/server.py",
            "use": "@sisp/python-wsgi",
            "config": {
                "maxLambdaSize": "15mb"
            }
        },
        {
            "src": "api/static/**",
            "use": "@now/static"
        }
    ],
    "routes": [{
            "src": "/",
            "dest": "/api/server.py"
        },
        {
            "src": "/hurwitz",
            "dest": "/api/server.py"
        }
    ]
}